	<!-- For TimePicker, DatePicker, ComboBox, ... -->
	<Style x:Key="PickerToggleButtonStyle" TargetType="ToggleButton">
		<Setter Property="IsTabStop" Value="False" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="ToggleButton">
					<Grid>
					
						<Border x:Name="Chrome"
								BorderBrush="{StaticResource ExpanderButtonNormal_Border}"
								BorderThickness="0">
							<Border x:Name="ChromeInner"
									BorderBrush="{StaticResource ExpanderButtonNormal_Background}"
									BorderThickness="1"
									Background="{StaticResource ExpanderButtonNormal_Background}" />
						</Border>

						<!-- checkin state-->
						<Border x:Name="ChromeCheck" Visibility="Collapsed"
								BorderBrush="{StaticResource ExpandButtonPressed_OuterBorder}"
								BorderThickness="0">
							<Border BorderBrush="{StaticResource ExpanderButtonPressed_Border}"
									BorderThickness="1"
									Background="{StaticResource ExpanderButtonPressed_Background}" />
						</Border>

						<Path x:Name="Path"
								Data="F1 M -2.54313e-006,-12L 1.33333,-12L 1.33333,-10.6667L 2.66667,-10.6667L 2.66667,-9.3334L 4,-9.3334L 4,-8.00001L 5.33333,-8.00001L 5.33333,-6.66667L 6.66667,-6.66667L 6.66667,-5.33334L 5.33333,-5.33334L 5.33333,-4.00001L 4,-4.00001L 4,-2.66673L 2.66667,-2.66673L 2.66667,-1.33341L 1.33333,-1.33341L 1.33333,0L -2.54313e-006,0L -2.54313e-006,-12 Z"
								Stretch="Fill" Width="5" Height="9"
								Fill="{StaticResource ExpandElement_NormalBrush}"
								HorizontalAlignment="Right" Margin="5,2,7,0"
								VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
							<Path.RenderTransform>
								<RotateTransform Angle="90" />
							</Path.RenderTransform>
						</Path>

						<ContentPresenter Content="{TemplateBinding Content}"
								ContentTemplate="{TemplateBinding ContentTemplate}" />

					</Grid>
				
					<ControlTemplate.Triggers>
						<!--<Trigger Property="IsKeyboardFocusWithin" Value="True">
							<Setter TargetName="InnerBorder" Property="BorderBrush"
									Value="{StaticResource FocusBrush}" />
						</Trigger>-->
						<Trigger Property="IsMouseOver" Value="true">
							<Setter TargetName="Chrome" Property="BorderBrush" 
													Value="{StaticResource ExpandButtonOver_OuterBorder}" />
							<Setter TargetName="Path" Property="Fill" 
													Value="{StaticResource ExpandElement_OverBrush}" />
							<Setter TargetName="ChromeInner" Property="BorderBrush" 
													Value="{StaticResource ExpanderButtonOver_Border}" />
							<Setter TargetName="ChromeInner" Property="Background" 
													Value="{StaticResource ExpanderButtonOver_Background}" />
						</Trigger>
						<Trigger Property="IsPressed" Value="True">
							<Setter TargetName="Chrome" Property="BorderBrush"
													Value="{StaticResource ExpandButtonPressed_OuterBorder}" />
							<Setter TargetName="Path" Property="Fill"
													Value="{StaticResource ExpandElement_PressBrush}" />
							<Setter TargetName="ChromeInner" Property="BorderBrush"
													Value="{StaticResource ExpanderButtonPressed_Border}" />
							<Setter TargetName="ChromeInner" Property="Background"
													Value="{StaticResource ExpanderButtonPressed_Background}" />
						</Trigger>
						<!--<Trigger Property="IsEnabled" Value="True">
							<Setter TargetName="DisabledVisual" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter TargetName="DisabledVisual" Property="Visibility" Value="Visible" />
						</Trigger>-->
						<Trigger Property="IsChecked" Value="True">
							<Setter TargetName="ChromeCheck" Property="Visibility" Value="Visible" />
							<Setter TargetName="Path" Property="Fill"
													Value="{StaticResource ExpandElement_PressBrush}" />
						</Trigger>
					</ControlTemplate.Triggers>
				
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
